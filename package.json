{
	"name": "waddle-bot",
	"type": "module",
	"version": "2.0.0",
	"description": "Multipurpose/Fun Discord Bot",
	"main": "dist/index.js",
	"scripts": {
		"start": "npx tsc && node dist/index.js",
		"dev": "nodemon dist/index.js",
		"watch": "npx tsc -w",
		"deploy": "npx tsc && node dist/deploy.js",
		"lint": "npx eslint src/**/*.ts"
	},
	"author": "https://github.com/BaumianerNiklas",
	"imports": {
		"#util/*": [
			"./dist/util/*"
		],
		"#structures/*": [
			"./dist/structures/*"
		],
		"#assets/*": [
			"./assets/*"
		]
	},
	"license": "ISC",
	"dependencies": {
		"@discordjs/rest": "^0.3.0",
		"canvas": "^2.8.0",
		"common-tags": "^1.8.0",
		"discord.js": "^14.0.0-dev.1648339713-520f471",
		"dotenv": "^8.2.0",
		"he": "^1.2.0",
		"mathjs": "^9.4.4",
		"ms": "^2.1.2",
		"node-fetch": "^3.0.0",
		"pino": "^6.13.0",
		"pino-pretty": "^5.1.2"
	},
	"devDependencies": {
		"@types/common-tags": "^1.8.1",
		"@types/he": "^1.1.2",
		"@types/ms": "^0.7.31",
		"@types/node": "^16.7.1",
		"@types/pino": "^6.3.11",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"eslint": "^7.32.0",
		"nodemon": "^2.0.13",
		"prettier": "^2.3.2",
		"typescript": "^4.4.4"
	}
}
